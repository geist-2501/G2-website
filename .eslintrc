{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "react/prop-types": "off",
    "react/jsx-one-expression-per-line": "off",
    "padded-blocks": "off",
    "max-len": "off",
    "react/sort-comp": "off"
  }
}
